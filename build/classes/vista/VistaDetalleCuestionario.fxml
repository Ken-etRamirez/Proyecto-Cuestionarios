<AnchorPane id="AnchorPane" prefHeight="380.0" prefWidth="534.0" style="-fx-background-color: #c6c6c6;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.VistaDetalleCuestionarioController">
   <children>
      <TableView fx:id="tblPreguntasCuestionario" layoutX="63.0" layoutY="110.0" prefHeight="200.0" prefWidth="395.0">
